    "version": "0.2.0",
    "configurations": [
        {
            "name": "FR: pyNetWatch in perso (DEBUG)",
            "type": "debugpy",
            "request": "launch",
            "program": "src/main.pyw",
            "console": "integratedTerminal",
            "args": "--config-file=perso\\config.json "
        },
        {
            "name": "EN: pyNetWatch in perso (DEBUG)",
            "type": "debugpy",
            "request": "launch",
            "program": "src/main.pyw",
            "console": "integratedTerminal",
            "args": "--config-file=perso\\config-en.json "
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}